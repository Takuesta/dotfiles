set colored-stats on

"\C-j": backward-word
# On Mac, Ctrl+o is not working by default. To enable, run `stty discard undef` command.
"\C-o": forward-word
"\C-g": kill-word
# "\C-l": clear-screen
"\C-x*": "'**'"

# On Mac, case-sensitive is default.
set completion-ignore-case on

